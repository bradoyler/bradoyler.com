{
  "name": "bradoyler.com",
  "version": "1.0.0",
  "description": "## How to Setup",
  "main": "index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "build-css": "npx tailwindcss -i ./src/assets/input.css -o ./docs/assets/style.css --minify",
    "watch-css": "npx tailwindcss -i ./src/assets/input.css -o ./docs/assets/style.css --watch",
    "build": "npm run build-css && eleventy",
    "start": "concurrently \"npm run watch-css\" \"eleventy --serve --watch\"",
    "dev": "npm run start"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^5.0.2",
    "@tailwindcss/typography": "^0.5.19",
    "autoprefixer": "^10.4.22",
    "concurrently": "^9.2.1",
    "handlebars": "^4.7.8",
    "handlebars-helpers": "^0.10.0",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typogr": "^0.6.8"
  },
  "dependencies": {
    "date-fns": "^4.1.0"
  }
}
